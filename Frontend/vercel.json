{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Frontend/index.html"
      }
    ]
  }
  